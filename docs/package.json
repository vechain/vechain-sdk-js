{
  "name": "docs",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "test:before": "docker compose up -d",
    "test:after": "docker compose down",
    "test:examples": "yarn test:before && ts-node-test examples/ && yarn test:after",
    "build": "find . -name \"*.md\" -type f -maxdepth 1 ! -name \"README.md\" -delete && tsup ./build-scripts && node dist/builddocs.cjs"
  },
  "dependencies": {
    "@vechainfoundation/vechain-sdk-core": "*",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "expect": "^29.7.0",
    "ts-node-test": "^0.4.3"
  }
}
