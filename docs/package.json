{
  "name": "docs",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "test:docs": "npx runme run --all --category example --skip-prompts",
    "test:examples": "ts-node-test examples/",
    "build": "find . -name \"*.md\" -type f -maxdepth 1 ! -name \"README.md\" -delete && node --loader ts-node/esm builddocs.ts"
  },
  "dependencies": {
    "@vechain-sdk/core": "*",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "expect": "^29.7.0",
    "runme": "^3.0.2",
    "ts-node": "^10.9.1",
    "ts-node-test": "^0.4.3"
  }
}
