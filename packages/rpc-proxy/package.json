{
    "name": "@vechain/vechain-sdk-rpc-proxy",
    "version": "1.0.0",
    "description": "Proxy Thor's RESTful API to Eth JSON-RPC",
    "author": "vechain Foundation",
    "license": "MIT",
    "keywords": [
        "vechain",
        "remix",
        "proxy"
    ],
    "private": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "rm -rf ./dist && tsup src/index.ts --format cjs,esm --dts",
        "lint": "eslint --ext .ts src --ext .ts",
        "format": "prettier --write src/**/*.ts",
        "start": "yarn build && node dist/index.js"
    },
    "dependencies": {
        "@vechain/vechain-sdk-network": "*",
        "@vechain/vechain-sdk-provider": "*",
        "@vechain/vechain-sdk-wallet": "*",
        "cors": "^2.8.5",
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.22"
    }
}
